            <value>RED</value>
            <value>WHITE</value>
            <value>BLUE</value>
            <value>PURPLE</value>
        </member>
        <member type="WorldSharedConfig*" name="WorldSharedConfig"/>
        <member type="bool" name="PerfMonitorEnabled" default="false"/>
        <member type="bool" name="VerifyServerData" default="false"/>
    </object>
</objgen>
